{
  "name": "nexusnetsoft/academy",
  "description": "Spryker Academy",
  "license": "MIT",
  "require": {
    "nexusnetsoftgmbh/nxscli-spryker": "^2.2.0"
  },
  "require-dev": {
    "xervice/development": "*"
  },
  "autoload": {
    "psr-4": {
      "DataProvider\\": "cli/Generated/"
    },
    "psr-0": {
      "Nexus\\": "cli/"
    }
  },
  "scripts": {
    "phpstan": [
      "php -d memory_limit=1024M current/vendor/bin/phpstan analyze -c current/phpstan.neon current/src/ -l 4"
    ]
  }
}
